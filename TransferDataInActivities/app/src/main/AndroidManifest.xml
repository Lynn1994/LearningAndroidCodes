<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lynn1994.transferdatainactivities">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".IntentWithSimpleData" />
        <activity android:name=".ShowDataDetail" />
        <activity android:name=".IntentWithBundleData" />
        <activity android:name=".IntentWithValueObject" />
        <activity android:name=".ShowBundleData" />
        <activity android:name=".ShowStringData" />
        <activity android:name=".ShowParcelableData" />
        <activity android:name=".ShowSerializableData" />
        <activity android:name=".SendBackData"></activity>
    </application>

</manifest>
